Algoritmo "exemploExercicio"
Var

   base, altura, area : real

Inicio

//    Trecho para calcular as medidas
   escreval("Qual a base?")
   leia(base)
   escreval("Qual a altura?")
   leia(altura)
//    Trecho para calcular a area
   area <- base*altura

//    Trecho para mostrar resultado
   escreval("A area é: ",area)

Fimalgoritmo